<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="view_style_base_title_bar">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="view_style_menu_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">#7986CB</item>
    </style>

    <style name="view_style_menu_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">#A8AAB4</item>
    </style>

    <style name="view_style_div">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/black</item>
    </style>
</resources>